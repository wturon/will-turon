{
  "compilerOptions": {
    "strict": false,
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "allowJs": true,
    "target": "ES5",
    "outDir": ".build",
    "moduleResolution": "node",
    "rootDir": "../infra",
    "baseUrl": "../infra",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noEmit": false
  },
  "exclude": ["node_modules", ".serverless/**/*", ".build/**/*"]
}
